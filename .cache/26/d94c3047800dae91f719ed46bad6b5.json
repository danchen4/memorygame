f somebody needs to print\r\n@mixin bp($breakpoint) {\r\n  @if $breakpoint == phone {\r\n    @media only screen and (max-width: 37.5em) {\r\n      //600px\r\n      @content;\r\n    }\r\n  }\r\n  @if $breakpoint == tab-port {\r\n    @media only screen and (max-width: 56.25em) {\r\n      //900px\r\n      @content;\r\n    }\r\n  }\r\n  @if $breakpoint == tab-land {\r\n    @media only screen and (max-width: 75em) {\r\n      //1200px\r\n      @content;\r\n    }\r\n  }\r\n  @if $breakpoint == big-desktop {\r\n    @media only screen and (min-width: 112.5em) {\r\n      //1800px\r\n      @content;\r\n    }\r\n  }\r\n}\r\n","..\\src\\sass\\base\\_base.scss":"*,\r\n*::after,\r\n*::before {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: inherit; /* better practice if you want to use different box-sizing*/\r\n}\r\n\r\nhtml {\r\n  // this defines what one 1rem is;\r\n  /* easier for REM calculation. Browser default is 16px;  Set as % to browser default */\r\n  font-size: 62.5%; //1 rem = 10px / 16px\r\n\r\n  @include bp(tab-land) {\r\n    // width < 1200\r\n    font-size: 56.25%; //1rem = 9px, 9/16 = 56.25%\r\n  }\r\n\r\n  @include bp(tab-port) {\r\n    //width < 900\r\n    font-size: 50%; //1rem = 8px, 8/16 = 50%\r\n  }\r\n\r\n  @include bp(big-desktop) {\r\n    //width > 1800\r\n    font-size: 75%; //1rem = 12, 12/16\r\n  }\r\n}\r\n\r\n/* set font */\r\nbody {\r\n  box-sizing: border-box;\r\n  padding: 3rem;\r\n\r\n  font-family: $font-main;\r\n  line-height: 1.7; /* 1.7x more than original line height */\r\n  color: $color-black;\r\n  letter-spacing: 1px;\r\n\r\n  background-color: $color-white;\r\n\r\n  @include bp(tab-port) {\r\n    padding: 0;\r\n  }\r\n}\r\n\r\n::selection {\r\n  background-color: $color-primary;\r\n  color: $color-white;\r\n}\r\n","..\\src\\sass\\abstracts\\_variables.scss":"// COLORS\r\n$color-primary: #55c57a;\r\n$color-primary-light: #7ed56f;\r\n$color-primary-dark: #28b485;\r\n\r\n$color-secondary-light: #ffb900;\r\n$color-secondary-dark: #ff7730;\r\n\r\n$color-tertiary-light: #2998ff;\r\n$color-tertiary-dark: #5643fa;\r\n\r\n$color-grey-light-1: #f9f9f9;\r\n$color-grey-light-2: #f7f7f7;\r\n$color-grey-light-3: #eeeeee;\r\n\r\n$color-grey-dark: #777;\r\n$color-grey-dark-2: #999;\r\n$color-grey-dark-3: #333;\r\n$color-white: #fff;\r\n$color-black: #000;\r\n\r\n// GRID\r\n$grid-width: 114rem;\r\n$grid-width-sm: 60rem;\r\n$gutter-vertical: 8rem;\r\n$gutter-vertical-sm: 6rem;\r\n$gutter-horizontal: 6rem;\r\n\r\n// FONTS\r\n$font-main: 'Luckiest Guy', cursive;\r\n$font-secondary: 'Montserrat', sans-serif;\r\n\r\n// FONT SIZES\r\n$default-font-size: 1.6rem;\r\n\r\n// BOX SHADOW\r\n$shadow-xs: 0 0.5rem 2rem rgba($color-black, 0.08);\r\n$shadow-sm: 0 0.8rem 2.5rem rgba($color-black, 0.1);\r\n$shadow-md: 0 1.5rem 4rem rgba($color-black, 0.15);\r\n$shadow-lg: 0 2rem 5rem rgba($color-black, 0.2);\r\n\r\n// BORDER RADIUS\r\n$border-radius-sm: 0.3rem;\r\n$border-radius-md: 0.6rem;\r\n$border-radius-lg: 1rem;\r\n\r\n// PANELS\r\n$panel-height: 15rem;\r\n\r\n$panel-height-phone: 10rem;\r\n","..\\src\\sass\\_layout.scss":".container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n\r\n  @include bp(phone) {\r\n    padding-top: 2rem;\r\n  }\r\n}\r\n\r\n.board {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr 1fr;\r\n  grid-gap: 2rem;\r\n  // width: 100%;\r\n  // height: 100%;\r\n\r\n  padding: 2rem;\r\n  background-color: $color-white;\r\n\r\n  @include bp(phone) {\r\n    grid-gap: 1rem;\r\n    padding: 0.5rem;\r\n  }\r\n}\r\n","..\\src\\sass\\components\\_button.scss":".btn {\r\n  &,\r\n  &:link,\r\n  &:visited {\r\n    text-transform: uppercase;\r\n    text-decoration: none;\r\n    padding: 1.5rem 4rem;\r\n    display: inline-block;\r\n    border-radius: 10rem;\r\n    transition: all 0.1s; /* transition: all on original state */\r\n    position: relative;\r\n    font-family: $font-main;\r\n    font-size: 2rem;\r\n\r\n    // Change for <button> element\r\n    border: none; //buttons have default border\r\n    cursor: pointed;\r\n  }\r\n\r\n  &:hover {\r\n    transform: translateY(-0.3rem);\r\n    box-shadow: 0 1rem 2rem rgba($color-black, 0.2);\r\n    cursor: pointer;\r\n\r\n    &::after {\r\n      transform: scaleX(1.4) scaleY(1.6);\r\n      opacity: 0;\r\n    }\r\n  }\r\n\r\n  &:active,\r\n  &:focus {\r\n    outline: none;\r\n    transform: translateY(-0.1rem);\r\n    box-shadow: 0 0.5rem 1rem rgba($color-black, 0.2);\r\n  }\r\n\r\n  &--white {\r\n    background-color: $color-white;\r\n    color: $color-grey-dark;\r\n\r\n    &::after {\r\n      background-color: $color-white;\r\n    }\r\n  }\r\n\r\n  &--primary {\r\n    background-color: $color-primary-dark;\r\n    color: white;\r\n\r\n    // &::after {\r\n    //   background-color: $color-primary-dark;\r\n    // }\r\n  }\r\n\r\n  /* On hover */\r\n\r\n  &--animated {\r\n    animation: fadeInBottom 0.8s ease-out 0.7s;\r\n    animation-fill-mode: backwards; /* applies 0% before animation starts */\r\n  }\r\n}\r\n\r\n.btn-text {\r\n  &:link,\r\n  &:visited {\r\n    font-size: $default-font-size;\r\n    color: $color-primary;\r\n    display: inline-block;\r\n    text-decoration: none;\r\n    border-bottom: 1px solid $color-primary;\r\n    padding: 3px;\r\n    transition: all 0.2s;\r\n  }\r\n\r\n  &:hover {\r\n    transform: translateY(-3px);\r\n    background-color: $color-primary-light;\r\n    color: $color-white;\r\n    box-shadow: 0 1rem 1rem rgba($color-black, 0.2);\r\n  }\r\n\r\n  &:active {\r\n    transform: translateY(0);\r\n    box-shadow: 0 0.5rem 0.5rem rgba($color-black, 0.2);\r\n  }\r\n}\r\n","..\\src\\sass\\components\\_panel.scss":".panel {\r\n  height: $panel-height;\r\n\r\n  // FUNCTIONALITY\r\n  perspective: 50rem; //add 'rotating' look\r\n  -moz-perspective: 50rem;\r\n  position: relative;\r\n\r\n  // height collapsed because of absolute position on children. (similiar to float, but can't be clear fixed)\r\n  height: $panel-height;\r\n\r\n  @include bp(phone) {\r\n    height: $panel-height-phone;\r\n  }\r\n\r\n  &__side {\r\n    height: $panel-height;\r\n    border: 1rem solid $color-white;\r\n    border-radius: $border-radius-lg;\r\n    box-shadow: $shadow-md;\r\n\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    backface-visibility: hidden;\r\n    transition: all 0.3s ease;\r\n\r\n    &--front {\r\n      background-color: $color-primary-dark;\r\n    }\r\n\r\n    &--back {\r\n      background-color: $color-grey-light-2;\r\n      transform: rotateY(180deg);\r\n    }\r\n\r\n    @include bp(phone) {\r\n      height: $panel-height-phone;\r\n    }\r\n  }\r\n\r\n  &__emoji {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    font-size: 8rem;\r\n\r\n    @include bp(phone) {\r\n      font-size: 5rem;\r\n    }\r\n  }\r\n}\r\n","..\\src\\sass\\components\\_message.scss":".message {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background-color: rgba($color-black, 0.8);\r\n  z-index: 9999;\r\n  opacity: 0;\r\n  visibility: hidden;\r\n\r\n  &--active {\r\n    opacity: 1;\r\n    visibility: visible;\r\n  }\r\n\r\n  &__content {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n\r\n    width: 50%;\r\n    padding: 2rem;\r\n    background-color: $color-grey-light-2;\r\n    border: 4px solid $color-primary-light;\r\n    box-shadow: $shadow-md;\r\n    border-radius: $border-radius-md;\r\n\r\n    @include bp(phone) {\r\n      width: 90%;\r\n      padding: 1rem;\r\n    }\r\n  }\r\n\r\n  &__header {\r\n    font-size: 3rem;\r\n    margin-bottom: 2rem;\r\n    text-align: center;\r\n\r\n    &--win {\r\n      color: $color-primary-light;\r\n      font-size: 4rem;\r\n    }\r\n\r\n    &--lose {\r\n      color: $color-secondary-dark;\r\n      font-size: 4rem;\r\n    }\r\n  }\r\n\r\n  &__selection {\r\n    text-align: center;\r\n    margin-bottom: 3rem;\r\n  }\r\n}\r\n","..\\src\\sass\\components\\_scoreboard.scss":".scoreboard {\r\n  font-size: 3rem;\r\n  margin-bottom: 2rem;\r\n  text-align: center;\r\n  line-height: 1;\r\n\r\n  @include bp(phone) {\r\n    font-size: 2rem;\r\n  }\r\n\r\n  &__rounds {\r\n    font-size: 4rem;\r\n    color: $color-secondary-light;\r\n    -webkit-text-stroke-width: 2px;\r\n    -webkit-text-stroke-color: $color-secondary-dark;\r\n    opacity: 1;\r\n    transition: all .4s ease-in;\r\n\r\n\r\n    @include bp(phone) {\r\n      font-size: 3rem;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"e62c3c614c672dca72e297475937d6ff","cacheData":{"env":{}}}